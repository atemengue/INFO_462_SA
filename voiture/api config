sudo docker build -t voiture .

sudo docker run -d -p 3307:3306 --name mysqldb -e MYSQL_ROOT_PASSWORD=root -e MYSQL_DATABASE=voiture mysql:latest

sudo docker network create spring-net

docker network connect spring-net mysqldb

sudo docker run -d -p 9090:8080 --name voiture --net spring-net -e MYSQL_HOST=mysqldb -e MYSQL_USER=root -e MYSQL_PASSWORD=root -e MYSQL_PORT=3306 voiture


